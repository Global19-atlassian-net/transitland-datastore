{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Transitland Datastore Stop",
  "type": "object",
  "required": ["onestopId"],
  "additionalProperties": false,
  "properties": {
    "onestopId": {
      "type": "string",
      "format": "stop-onestop-id"
    },
    "geometry": { "type": "object" },
    "timezone": { "type": "string" },
    "tags": { "type": "object" },
    "name": { "type": "string" },
    "parentStopOnestopId": {
      "type": ["string", "null"]
    },
    "identifiedBy": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "notIdentifiedBy": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "servedBy": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string",
        "format": "operator-onestop-id"
      }
    },
    "notServedBy": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string",
        "format": "operator-onestop-id"
      }
    }
  }
}
